#include <stdio.h>
int main() {
    int n, x, sum = 0;
    printf("How many numbers: ");
    scanf("%d", &n);
    for(int i=1; i<=n; i++){
        printf("Enter number %d: ", i);
        scanf("%d", &x);
        if(x<0) printf("Skipping negative number!\n");
        else sum += x;
    }
    printf("Sum = %d", sum);
    return 0;
