#include <stdio.h>
int main() {
    int n, i, temp, sum = 0;
    printf("Enter n: ");
    scanf("%d", &n);
    for(i = 1; i <= n; i++) {
        temp = i;
        while(temp > 0) {
            sum += temp % 10;
            temp /= 10;
        }
    }
    printf("Sum of all digits = %d", sum);
    return 0;
}
