#include <stdio.h>
int main() {
    int n, temp, d, sum = 0, count = 0;
    printf("Enter number: ");
    scanf("%d", &n);
    temp = n;
    while(temp > 0) { count++; temp /= 10; }
    temp = n;
    while(temp > 0) {
        d = temp % 10;
        int pow = 1;
        for(int i=0; i<count; i++) pow *= d;
        sum += pow;
        temp /= 10;
    }
    printf("Sum of powered digits = %d", sum);
    return 0;
}
