#include <stdio.h>
int main() {
    int n, num, sum=0;
    printf("Enter total numbers: ");
    scanf("%d", &n);
    for(int i=1;i<=n;i++){
        printf("Enter number %d: ", i);
        scanf("%d", &num);
        if(i%2==0) sum+=num;
    }
    printf("Sum of even index numbers = %d", sum);
    return 0;
}
