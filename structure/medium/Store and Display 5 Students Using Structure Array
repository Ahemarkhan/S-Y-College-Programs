#include <stdio.h>

struct Student {
    int roll;
    char name[20];
    float marks;
};

int main() {
    struct Student s[5];
    for(int i=0;i<5;i++) {
        printf("Enter roll, name, marks: ");
        scanf("%d %s %f", &s[i].roll, s[i].name, &s[i].marks);
    }
    printf("\nStudent List:\n");
    for(int i=0;i<5;i++)
        printf("%d %s %.2f\n", s[i].roll, s[i].name, s[i].marks);
    return 0;
}
