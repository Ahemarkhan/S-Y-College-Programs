#include <stdio.h>

struct Student {
    int roll;
    char name[20];
    float marks;
};

int main() {
    struct Student s1 = {1,"Amit",90.5}, s2;
    FILE *f = fopen("student.dat","wb");
    fwrite(&s1,sizeof(s1),1,f);
    fclose(f);

    f = fopen("student.dat","rb");
    fread(&s2,sizeof(s2),1,f);
    fclose(f);

    printf("From File: %d %s %.2f\n", s2.roll, s2.name, s2.marks);
    return 0;
}
