#include <stdio.h>

struct Student {
    int roll;
    char name[20];
    float marks;
};

int main() {
    struct Student s[3];
    for(int i=0;i<3;i++)
        scanf("%d %s %f", &s[i].roll, s[i].name, &s[i].marks);

    int maxIndex=0;
    for(int i=1;i<3;i++)
        if(s[i].marks > s[maxIndex].marks)
            maxIndex = i;

    printf("Topper: %s (%.2f)\n", s[maxIndex].name, s[maxIndex].marks);
    return 0;
}
