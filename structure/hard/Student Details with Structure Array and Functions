#include <stdio.h>

struct Student {
    int roll;
    char name[20];
    float marks;
};

void display(struct Student s[], int n) {
    for(int i=0;i<n;i++)
        printf("%d %s %.2f\n", s[i].roll, s[i].name, s[i].marks);
}

int main() {
    struct Student s[3];
    for(int i=0;i<3;i++)
        scanf("%d %s %f", &s[i].roll, s[i].name, &s[i].marks);
    display(s,3);
    return 0;
}
