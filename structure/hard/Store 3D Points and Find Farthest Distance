#include <stdio.h>
#include <math.h>

struct Point {
    float x, y, z;
};

float distance(struct Point p) {
    return sqrt(p.x*p.x + p.y*p.y + p.z*p.z);
}

int main() {
    struct Point p[3];
    for(int i=0;i<3;i++)
        scanf("%f %f %f", &p[i].x, &p[i].y, &p[i].z);

    int far = 0;
    for(int i=1;i<3;i++)
        if(distance(p[i]) > distance(p[far])) far = i;

    printf("Farthest Point: (%.2f, %.2f, %.2f)\n", p[far].x, p[far].y, p[far].z);
    return 0;
}
