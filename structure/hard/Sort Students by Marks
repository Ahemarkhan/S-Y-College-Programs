#include <stdio.h>

struct Student {
    char name[20];
    float marks;
};

int main() {
    struct Student s[5] = {{"Ravi",75},{"Amit",88},{"Sita",60},{"Neha",90},{"Raju",70}}, temp;
    for(int i=0;i<5;i++)
        for(int j=i+1;j<5;j++)
            if(s[i].marks < s[j].marks) {
                temp=s[i];
                s[i]=s[j];
                s[j]=temp;
            }

    printf("Sorted by Marks:\n");
    for(int i=0;i<5;i++)
        printf("%s %.2f\n", s[i].name, s[i].marks);
    return 0;
}
