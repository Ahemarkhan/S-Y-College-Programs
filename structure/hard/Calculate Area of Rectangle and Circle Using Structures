#include <stdio.h>
#define PI 3.14

struct Rectangle {
    float length, width;
};

struct Circle {
    float radius;
};

int main() {
    struct Rectangle r = {5, 4};
    struct Circle c = {3};

    printf("Area of Rectangle: %.2f\n", r.length * r.width);
    printf("Area of Circle: %.2f\n", PI * c.radius * c.radius);
    return 0;
}
