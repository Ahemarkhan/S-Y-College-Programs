#include <stdio.h>
int search(int a[], int n, int key){
    for(int i=0;i<n;i++)
        if(a[i]==key) return i;
    return -1;
}
int main(){
    int a[5]={1,2,3,4,5}, key=4;
    int pos=search(a,5,key);
    if(pos!=-1) printf("Found at index %d", pos);
    else printf("Not found");
    return 0;
}
