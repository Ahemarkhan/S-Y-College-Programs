#include <stdio.h>
int diagonalSum(int a[10][10], int n){
    int s=0;
    for(int i=0;i<n;i++) s+=a[i][i];
    return s;
}
int main(){
    int n,a[10][10];
    scanf("%d",&n);
    for(int i=0;i<n;i++)
        for(int j=0;j<n;j++)
            scanf("%d",&a[i][j]);
    printf("Diagonal Sum=%d",diagonalSum(a,n));
    return 0;
}
