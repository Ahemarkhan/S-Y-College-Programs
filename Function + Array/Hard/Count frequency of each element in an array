#include <stdio.h>
void freq(int a[], int n){
    int i,j,count,visited[100]={0};
    for(i=0;i<n;i++){
        if(visited[i]) continue;
        count=1;
        for(j=i+1;j<n;j++)
            if(a[i]==a[j]){
                count++;
                visited[j]=1;
            }
        printf("%d occurs %d times\n",a[i],count);
    }
}
int main(){
    int n,a[100];
    scanf("%d",&n);
    for(int i=0;i<n;i++) scanf("%d",&a[i]);
    freq(a,n);
    return 0;
}
