#include <stdio.h>
int diagSum(int a[10][10], int n){
    int s=0;
    for(int i=0;i<n;i++)
        s+=a[i][i]+a[i][n-i-1];
    if(n%2!=0) s-=a[n/2][n/2];
    return s;
}
int main(){
    int n,a[10][10];
    scanf("%d",&n);
    for(int i=0;i<n;i++)
        for(int j=0;j<n;j++)
            scanf("%d",&a[i][j]);
    printf("Diagonal Sum=%d",diagSum(a,n));
    return 0;
}
