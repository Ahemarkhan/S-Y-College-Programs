#include <stdio.h>
int isSorted(int a[], int n){
    for(int i=0;i<n-1;i++)
        if(a[i]>a[i+1])
            return 0;
    return 1;
}
int main(){
    int n,a[50];
    scanf("%d",&n);
    for(int i=0;i<n;i++) scanf("%d",&a[i]);
    if(isSorted(a,n))
        printf("Array is sorted");
    else
        printf("Array not sorted");
    return 0;
}
