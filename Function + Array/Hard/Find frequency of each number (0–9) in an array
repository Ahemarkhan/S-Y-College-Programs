#include <stdio.h>
void countFreq(int a[], int n){
    int freq[10]={0};
    for(int i=0;i<n;i++)
        if(a[i]>=0 && a[i]<=9)
            freq[a[i]]++;
    for(int i=0;i<10;i++)
        if(freq[i]>0)
            printf("%d -> %d times\n",i,freq[i]);
}
int main(){
    int n,a[50];
    scanf("%d",&n);
    for(int i=0;i<n;i++) scanf("%d",&a[i]);
    countFreq(a,n);
    return 0;
}
