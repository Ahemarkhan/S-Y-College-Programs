#include <stdio.h>
int sumEven(int a[], int n){
    int s=0;
    for(int i=0;i<n;i++)
        if(a[i]%2==0) s+=a[i];
    return s;
}
int sumOdd(int a[], int n){
    int s=0;
    for(int i=0;i<n;i++)
        if(a[i]%2!=0) s+=a[i];
    return s;
}
int main(){
    int n,a[50];
    scanf("%d",&n);
    for(int i=0;i<n;i++) scanf("%d",&a[i]);
    printf("Even Sum=%d\n",sumEven(a,n));
    printf("Odd Sum=%d",sumOdd(a,n));
    return 0;
}
