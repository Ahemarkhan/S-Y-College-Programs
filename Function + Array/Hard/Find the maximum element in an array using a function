#include <stdio.h>
int max(int a[], int n) {
    int i, max = a[0];
    for(i = 1; i < n; i++)
        if(a[i] > max)
            max = a[i];
    return max;
}
int main() {
    int n, i, a[100];
    printf("Enter size: ");
    scanf("%d", &n);
    for(i = 0; i < n; i++)
        scanf("%d", &a[i]);
    printf("Max = %d", max(a, n));
    return 0;
}
