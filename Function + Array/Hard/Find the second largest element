#include <stdio.h>
int secondLargest(int a[], int n){
    int i, first=-9999, second=-9999;
    for(i=0;i<n;i++){
        if(a[i]>first){
            second=first;
            first=a[i];
        }
        else if(a[i]>second && a[i]<first)
            second=a[i];
    }
    return second;
}
int main(){
    int n,a[50];
    scanf("%d",&n);
    for(int i=0;i<n;i++) scanf("%d",&a[i]);
    printf("Second Largest=%d",secondLargest(a,n));
    return 0;
}
