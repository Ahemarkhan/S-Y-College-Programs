#include <stdio.h>
int min(int a[], int n) {
    int i, m = a[0];
    for(i = 1; i < n; i++)
        if(a[i] < m)
            m = a[i];
    return m;
}
int main() {
    int n, a[50], i;
    printf("Enter size: ");
    scanf("%d", &n);
    for(i=0;i<n;i++) scanf("%d",&a[i]);
    printf("Min = %d", min(a,n));
    return 0;
}


