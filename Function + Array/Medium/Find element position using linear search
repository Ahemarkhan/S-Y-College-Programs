#include <stdio.h>
int linearSearch(int a[], int n, int key){
    for(int i=0;i<n;i++)
        if(a[i]==key) return i;
    return -1;
}
int main(){
    int a[6]={10,20,30,40,50,60}, key=30;
    int pos=linearSearch(a,6,key);
    if(pos!=-1) printf("Found at index %d", pos);
    else printf("Not found");
    return 0;
}
