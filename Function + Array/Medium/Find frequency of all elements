#include <stdio.h>
void frequency(int a[], int n){
    int visited[n];
    for(int i=0;i<n;i++) visited[i]=0;
    for(int i=0;i<n;i++){
        if(visited[i]==1) continue;
        int count=1;
        for(int j=i+1;j<n;j++)
            if(a[i]==a[j]){
                count++;
                visited[j]=1;
            }
        printf("%d occurs %d times\n", a[i], count);
    }
}
int main(){
    int a[8]={1,2,2,3,4,1,3,2};
    frequency(a,8);
    return 0;
}
