#include <stdio.h>
int main() {
    int n;
    printf("Enter a number: ");
    scanf("%d", &n);

    if(n % 3 == 0 && n % 7 == 0)
        printf("Divisible by both 3 and 7");
    else if(n % 3 == 0)
        printf("Divisible by 3 only");
    else if(n % 7 == 0)
        printf("Divisible by 7 only");
    else
        printf("Not divisible by 3 or 7");
    return 0;
}
