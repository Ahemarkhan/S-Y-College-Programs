#include <stdio.h>
#include <string.h>
#include <ctype.h>

int main() {
    char str[100];
    int digits = 0, alphabets = 0, special = 0;
    printf("Enter a string: ");
    gets(str);
    for (int i = 0; str[i] != '\0'; i++) {
        if (isalpha(str[i])) alphabets++;
        else if (isdigit(str[i])) digits++;
        else special++;
    }
    printf("Alphabets: %d, Digits: %d, Special: %d", alphabets, digits, special);
    return 0;
}
