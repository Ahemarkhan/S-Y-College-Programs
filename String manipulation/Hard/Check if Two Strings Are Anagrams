#include <stdio.h>
#include <string.h>

void sortString(char *s) {
    for (int i = 0; s[i]; i++)
        for (int j = i + 1; s[j]; j++)
            if (s[i] > s[j]) {
                char t = s[i];
                s[i] = s[j];
                s[j] = t;
            }
}

int main() {
    char a[50], b[50];
    printf("Enter first string: ");
    gets(a);
    printf("Enter second string: ");
    gets(b);

    sortString(a);
    sortString(b);

    if (strcmp(a, b) == 0)
        printf("Anagrams!\n");
    else
        printf("Not Anagrams.\n");
    return 0;
}
