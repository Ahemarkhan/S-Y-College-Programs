#include <stdio.h>
#include <string.h>

int main() {
    char email[100];
    printf("Enter email ID: ");
    gets(email);

    char *ptr = strchr(email, '@');
    if (ptr) {
        *ptr = '\0';
        printf("Username: %s\nDomain: %s\n", email, ptr + 1);
    } else
        printf("Invalid email.\n");
    return 0;
}
