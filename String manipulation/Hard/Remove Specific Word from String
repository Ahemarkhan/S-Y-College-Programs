#include <stdio.h>
#include <string.h>

int main() {
    char str[200], word[50];
    printf("Enter string: ");
    gets(str);
    printf("Word to remove: ");
    gets(word);

    char *token = strtok(str, " ");
    while (token) {
        if (strcmp(token, word) != 0)
            printf("%s ", token);
        token = strtok(NULL, " ");
    }
    return 0;
}
