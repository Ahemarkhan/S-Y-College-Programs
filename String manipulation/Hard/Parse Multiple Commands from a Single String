#include <stdio.h>
#include <string.h>

int main() {
    char input[200];
    printf("Enter commands (use ; as separator): ");
    gets(input);

    char *token = strtok(input, ";");
    while (token) {
        printf("Executing: %s\n", token);
        token = strtok(NULL, ";");
    }
    return 0;
}
