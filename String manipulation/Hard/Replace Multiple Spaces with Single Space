#include <stdio.h>
#include <string.h>

int main() {
    char str[200];
    printf("Enter string: ");
    gets(str);

    int j = 0;
    for (int i = 0; str[i]; i++)
        if (!(str[i] == ' ' && str[i + 1] == ' '))
            str[j++] = str[i];
    str[j] = '\0';

    printf("Cleaned string: %s\n", str);
    return 0;
}
