#include <stdio.h>
#include <string.h>

int main() {
    char str[200], words[50][50];
    printf("Enter a sentence: ");
    gets(str);
    int n = 0;

    char *token = strtok(str, " ");
    while (token != NULL) {
        strcpy(words[n++], token);
        token = strtok(NULL, " ");
    }

    printf("Reversed order: ");
    for (int i = n - 1; i >= 0; i--)
        printf("%s ", words[i]);
    return 0;
}
