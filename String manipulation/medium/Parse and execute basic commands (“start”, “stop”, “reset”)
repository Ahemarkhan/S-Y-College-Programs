#include <stdio.h>
#include <string.h>

int main() {
    char cmd[50];
    printf("Enter command: ");
    gets(cmd);

    if (strcmp(cmd, "start") == 0)
        printf("Robot Started!\n");
    else if (strcmp(cmd, "stop") == 0)
        printf("Robot Stopped!\n");
    else if (strcmp(cmd, "reset") == 0)
        printf("System Reset!\n");
    else
        printf("Invalid Command!\n");

    return 0;
}
