#include <stdio.h>
#include <string.h>
#include <ctype.h>

int main() {
    char str[200], result[200];
    int j = 0;
    printf("Enter a sentence: ");
    gets(str);
    for (int i = 0; str[i]; i++)
        if (isalnum(str[i]) || isspace(str[i]))
            result[j++] = str[i];
    result[j] = '\0';
    printf("Without punctuation: %s\n", result);
    return 0;
}
