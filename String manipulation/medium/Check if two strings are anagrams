#include <stdio.h>
#include <string.h>

void sort(char *s) {
    char temp;
    int n = strlen(s);
    for (int i = 0; i < n-1; i++)
        for (int j = i+1; j < n; j++)
            if (s[i] > s[j]) {
                temp = s[i]; s[i] = s[j]; s[j] = temp;
            }
}

int main() {
    char s1[50], s2[50];
    printf("Enter first string: ");
    gets(s1);
    printf("Enter second string: ");
    gets(s2);
    sort(s1); sort(s2);
    if (strcmp(s1, s2) == 0)
        printf("Anagram strings\n");
    else
        printf("Not anagrams\n");
    return 0;
}
