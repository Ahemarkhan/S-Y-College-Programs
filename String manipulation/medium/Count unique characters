#include <stdio.h>
#include <string.h>

int main() {
    char str[100];
    int freq[256] = {0}, unique = 0;
    printf("Enter a string: ");
    gets(str);
    for (int i = 0; str[i]; i++)
        freq[(unsigned char)str[i]]++;
    for (int i = 0; i < 256; i++)
        if (freq[i] == 1)
            unique++;
    printf("Unique characters: %d\n", unique);
    return 0;
}
