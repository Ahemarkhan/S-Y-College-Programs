#include <stdio.h>
#include <string.h>
int main() {
    char s[] = "Hello World";
    int v=0,cnt=0;
    for(int i=0;i<strlen(s);i++){
        char ch = s[i];
        if((ch>='A'&&ch<='Z')||(ch>='a'&&ch<='z')){
            char l = (ch>='A'&&ch<='Z') ? ch+32 : ch;
            if(l=='a'||l=='e'||l=='i'||l=='o'||l=='u') v++; else cnt++;
        }
    }
    printf("Vowels=%d Consonants=%d", v, cnt);
    return 0;
}
