#include <stdio.h>
int main() {
    int A[] = {1,3,5,7}, B[] = {2,4,6,8}, n=4, m=4;
    int C[8], i=0,j=0,k=0;
    for(; i<n && j<m; ){
        if(A[i]<=B[j]) C[k++]=A[i++]; else C[k++]=B[j++];
    }
    for(;i<n;i++) C[k++]=A[i];
    for(;j<m;j++) C[k++]=B[j];
    for(int t=0;t<k;t++) printf("%d ", C[t]);
    return 0;
}
