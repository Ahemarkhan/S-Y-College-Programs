#include <stdio.h>
int main() {
    int choice;
    float t;
    printf("1.C->F 2.F->C: ");
    scanf("%d", &choice);
    printf("Enter temperature: ");
    scanf("%f", &t);
    switch(choice) {
        case 1: printf("Fahrenheit = %.2f\n", (t*9/5)+32); break;
        case 2: printf("Celsius = %.2f\n", (t-32)*5/9); break;
        default: printf("Invalid");
    }
    return 0;
}
