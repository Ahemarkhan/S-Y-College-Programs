#include <stdio.h>
int main() {
    int ch, score=0;
    printf("Q1: 2+2=? 1)3 2)4 3)5\n");
    scanf("%d",&ch);
    switch(ch){ case 2: score++; }
    printf("Q2: Capital of Japan? 1)Seoul 2)Beijing 3)Tokyo\n");
    scanf("%d",&ch);
    switch(ch){ case 3: score++; }
    printf("Q3: 5*3=? 1)15 2)20 3)25\n");
    scanf("%d",&ch);
    switch(ch){ case 1: score++; }
    printf("Final Score: %d/3\n", score);
    return 0;
}
