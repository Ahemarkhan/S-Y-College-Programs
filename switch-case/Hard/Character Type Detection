#include <stdio.h>
int main() {
    char ch;
    printf("Enter a character: "); scanf(" %c",&ch);
    switch((ch>='A'&&ch<='Z')?1:(ch>='a'&&ch<='z')?2:(ch>='0'&&ch<='9')?3:4) {
        case 1: printf("Uppercase Letter\n"); break;
        case 2: printf("Lowercase Letter\n"); break;
        case 3: printf("Digit\n"); break;
        default: printf("Special Symbol\n");
    }
    return 0;
}
